---
import HeroSection from "../components/sections/HeroSection.astro";
import Layout from "../layouts/Layout.astro";
import BenefitsSection from "../components/sections/BenefitsSection/BenefitsSection.astro";
import SectionContainer from "../components/ui/SectionContainer.astro";
import ProgramsSection from "../components/sections/ProgramsSection/ProgramsSection.astro";
import Carousel from "../components/Carousel.jsx";
import Footer from "../components/Footer.astro";
import IntroAnimation from "../components/ui/IntroAnimation";
---

<Layout>
  <IntroAnimation client:only="load" onComplete={false} />
  <HeroSection />

  <SectionContainer>
    <BenefitsSection />
  </SectionContainer>

  <SectionContainer>
    <Carousel client:only="load" />
  </SectionContainer>

  <SectionContainer>
    <ProgramsSection />
  </SectionContainer>

  <SectionContainer>
    <Footer />
  </SectionContainer>
</Layout>
